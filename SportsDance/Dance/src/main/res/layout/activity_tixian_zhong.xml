<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:background="#f3f2f7"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <ImageView
        android:layout_marginTop="@dimen/spacing_260"
        android:layout_width="match_parent"
        android:layout_height="@dimen/spacing_600"
        android:layout_marginLeft="@dimen/spacing_20"
        android:layout_marginRight="@dimen/spacing_20"
        android:src="@drawable/tixian_back"
        android:id="@+id/imageView3"/>
       <TextView
           style="@style/tixian_zhong"
           android:text="提现处理中"
           android:gravity="center"
           android:layout_alignTop="@+id/imageView3"
           android:layout_marginTop="@dimen/spacing_160"/>
    <TextView
        android:layout_alignTop="@+id/imageView3"
        style="@style/tixian_zhong"
        android:layout_marginTop="@dimen/spacing_230"
        android:text="您的提现金额为"
        android:id="@+id/textView"/>
    <TextView
        android:id="@+id/tixian_money"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="@dimen/textsize36"
        android:textColor="#e53446"
        android:layout_below="@+id/textView"
        android:layout_marginTop="@dimen/spacing_20"/>
    <TextView
        style="@style/tixian_zhong"

        android:textSize="@dimen/textsize36"
        android:layout_below="@+id/tixian_money"
        android:layout_marginTop="@dimen/spacing_20"
        android:id="@+id/tixian_time"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/textsize24"
        android:textColor="#666666"
        android:text="我们会在1-7个工作日内为您处理该申请"
        android:gravity="center"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/spacing_100"/>
</RelativeLayout>